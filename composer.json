{
    "name": "[__OWNER__]/[__PACKAGE_NAME__]",
    "description": "[__DESCRIPTION__]",
    "keywords": [
        "[__OWNER__]",
        "[__PACKAGE_NAME__]",
        "laravel"
    ],
    "homepage": "https://github.com/[__OWNER__]/[__PACKAGE_NAME__]",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Reece May",
            "email": "zsh.rce@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^7.9|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "[__OWNER__]\\[__PACKAGE__]\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "[__OWNER__]\\[__PACKAGE__]\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "[__OWNER__]\\[__PACKAGE__]\\ServiceProvider"
            ]
        }
    }
}
